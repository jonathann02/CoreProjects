{
  "name": "graph-er-api",
  "version": "0.1.0",
  "description": "Graph & Entity Resolution API - Express + Apollo GraphQL + REST",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:int": "jest --testPathPattern=integration",
    "clean": "rm -rf dist"
  },
  "keywords": ["graphql", "entity-resolution", "neo4j", "typescript"],
  "author": "Graph & Entity Resolution Lab",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@as-integrations/express4": "^1.1.1",
    "@godaddy/terminus": "^4.12.1",
    "@graph-er/shared": "file:../../packages/shared",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.48.0",
    "@opentelemetry/exporter-otlp-http": "^0.48.0",
    "@opentelemetry/resources": "^1.24.1",
    "@opentelemetry/sdk-node": "^0.48.0",
    "@opentelemetry/semantic-conventions": "^1.24.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.24.0",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.6.0",
    "typescript": "~5.9.3"
  }
}
