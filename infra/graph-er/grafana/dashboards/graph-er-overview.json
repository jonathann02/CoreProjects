{
  "dashboard": {
    "id": null,
    "title": "Graph & Entity Resolution Lab - Overview",
    "tags": ["graph-er", "neo4j", "entity-resolution"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{status_code}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "API Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "Neo4j Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "neo4j_driver_pool_size",
            "legendFormat": "Pool size"
          },
          {
            "expr": "neo4j_driver_pool_in_use",
            "legendFormat": "In use"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 4,
        "title": "ETL Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(etl_records_processed_total[5m])",
            "legendFormat": "Records/sec"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 5,
        "title": "Match Quality Distribution",
        "type": "histogram",
        "targets": [
          {
            "expr": "match_quality_score",
            "legendFormat": "Match scores"
          }
        ],
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
      }
    ],
    "time": { "from": "now-1h", "to": "now" },
    "timepicker": {},
    "templating": { "list": [] },
    "annotations": { "list": [] },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
